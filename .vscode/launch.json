{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch NestJS",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/node_modules/.bin/nest",
			"args": ["start", "--watch"],
			"env": {
				"TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
			},
			"console": "integratedTerminal"
		}
	]
}
